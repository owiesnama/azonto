<modal name="videoPlay"
       width="50%"
       height="auto"
       :pivot-x=".5"
       :pivot-y=".5"
       classes="">
    <div class="card">
        <div class="card-header card-header-info">
            <h4 class="card-title">Video Player</h4>
        </div>
        <div class="card-body">
            <video class="video-js vjs-default-skin"
                   controls
                   autoplay
                   width="640" height="264"
                   data-setup='{ "techOrder": ["youtube"], "sources": [{ "type": "video/youtube", "src": "https://youtu.be/k-RtYiiB47E"}] }'>
            </video>
        </div>
    </div>
</modal>